<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
    <dubbo:application name="009-zk-multi-provider"></dubbo:application>
    <dubbo:protocol name="dubbo" port="20880"></dubbo:protocol>
    <dubbo:registry address="zookeeper://192.168.153.128:2181"></dubbo:registry>

    <dubbo:service interface="com.wzp.dubbo.service.UserService" ref="userService1" version="1.0.0"></dubbo:service>
    <dubbo:service interface="com.wzp.dubbo.service.UserService" ref="userService2" version="2.0.0"></dubbo:service>



    <bean id="userService1" class="com.wzo.dubbo.service.impl.UserServiceImpl"></bean>
    <bean id="userService2" class="com.wzo.dubbo.service.impl.UserServiceImpl2"></bean>



</beans>